{% extends '@FgimenezAdminlte/layout.admin.html.twig' %}

{% block title %}Correccion{% endblock %}

{% block body %}
    <h1>Correccion</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ correccion.id }}</td>
            </tr>
            <tr>
                <th>Monto</th>
                <td><i class="fas fa-dollar-sign"></i> {{ correccion.monto }}</td>
            </tr>
            <tr>
                <th>Observacion</th>
                <td>{{ correccion.observacion }}</td>
            </tr>
            <tr>
                <th>Observacion_sistema</th>
                <td>{{ correccion.observacionSistema }}</td>
            </tr>
            <tr>
                <th>CreatedBy</th>
                <td>{{ correccion.createdBy }}</td>
            </tr>
            <tr>
                <th>UpdatedBy</th>
                <td>{{ correccion.updatedBy }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ correccion.createdAt ? correccion.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ correccion.updatedAt ? correccion.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('correccion_index') }}">

        <i class="fas fa-arrow-left"></i>
        back to list

    </a>

    <a href="{{ path('correccion_edit', {'id': correccion.id}) }}">
        
         <i class="fas fa-edit"></i>
        edit

    </a>

    {{ include('correccion/_delete_form.html.twig') }}
{% endblock %}
