
{% block stylesheets %}
 <link rel="stylesheet" href="/bundles/fgimenezadminlte/plugins/datatables-bs4/css/dataTables.bootstrap4.css">
{% endblock %}


{% block body %}
    <h1>AdjuntoPago </h1>



           <div class="card-header">
              <h3 class="card-title"></h3>
            </div>
            <!-- /.card-header -->
            <div class="card-body">
     <table id="example2" class="table table-bordered table-hover">
        <thead>
            <tr>
                <th>Id</th>
                <th>Ruta</th>
                <th>Monto</th>
                <th>Validado_by</th>
                <th>Numero_referencia</th>
                <th>Observacion</th>
                <th>CreatedBy</th>
                <th>UpdatedBy</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                
            </tr>
        </thead>
        <tbody>
        {% for adjunto_pago in adjunto_pagos %}
            <tr>
                <td>{{ adjunto_pago.id }}</td>
                <td>{{ adjunto_pago.ruta }}</td>
                <td>{{ adjunto_pago.monto }}</td>
                <td>{{ adjunto_pago.validadoBy }}</td>
                <td>{{ adjunto_pago.numeroReferencia }}</td>
                <td>{{ adjunto_pago.observacion }}</td>
                <td>{{ adjunto_pago.createdBy }}</td>
                <td>{{ adjunto_pago.updatedBy }}</td>
                <td>{{ adjunto_pago.createdAt ? adjunto_pago.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ adjunto_pago.updatedAt ? adjunto_pago.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                
            </tr>
        {% else %}
            <tr>
                <td colspan="11">no records found</td>
            </tr>
        {% endfor %}
        </tbody>

        <tfoot>
            <tr>
                <th>Id</th>
                <th>Ruta</th>
                <th>Monto</th>
                <th>Validado_by</th>
                <th>Numero_referencia</th>
                <th>Observacion</th>
                <th>CreatedBy</th>
                <th>UpdatedBy</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
               
            </tr>
        </tfoot>
    </table>

  
    </div>

{% endblock %}
