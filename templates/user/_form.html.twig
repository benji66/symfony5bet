

{{ form_start(form) }}

	 			<div class="form-group">
                  {{ form_label(form.email,'Correo:') }}

                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fas fa-at"></i></span>
                    </div>
                    {{ form_widget(form.email) }}
    				        <small>{{ form_help(form.email) }}</small>
                  </div>
                  <!-- /.input group -->
                    
          </div>

          <div class="form-group">
                  {{ form_label(form.telefono,'Telefono:') }}

                  <div class="input-group">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fas fa-phone"></i></span>
                    </div>
                    {{ form_widget(form.telefono) }}
                    <small>{{ form_help(form.telefono) }}</small>
                  </div>
                  <!-- /.input group -->
                    
          </div>

                <div class="form-group">
                  <div class="input-group">                    
                    {{ form_widget(form.password) }}
                    
    				<small>{{ form_help(form.password) }}</small>
                  </div>
                  <!-- /.input group -->
                  <div class="form-error">				    
				    </div>
                </div>
     
     {{ form_rest(form) }}




     <div class="card-footer">
                <button type="submit" class="btn btn-primary">
                  {{ button_label|default('Registrar') }}
              </button>
     </div>
{{ form_end(form) }}
