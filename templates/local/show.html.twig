{% extends '@FgimenezAdminlte/layout.admin.html.twig' %}

{% block title %}Local{% endblock %}

{% block body %}
    <h1>Local</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ local.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ local.nombre }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ local.descripcion }}</td>
            </tr>
            <tr>
                <th>CreatedBy</th>
                <td>{{ local.createdBy }}</td>
            </tr>
            <tr>
                <th>UpdatedBy</th>
                <td>{{ local.updatedBy }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ local.createdAt ? local.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ local.updatedAt ? local.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('local_index') }}">

        <i class="fas fa-arrow-left"></i>
        back to list

    </a>

    <a href="{{ path('local_edit', {'id': local.id}) }}">
        
         <i class="fas fa-edit"></i>
        edit

    </a>

    {{ include('local/_delete_form.html.twig') }}
{% endblock %}
